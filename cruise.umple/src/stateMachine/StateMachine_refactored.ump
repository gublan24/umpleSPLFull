							return null;
						}
						else if(sm!=null&&smti.getTraceStateMachineFlag())
						{ 
							return smti;
						}
						else if(method.equals("transition"))
						{
							if(smti.getIsOut()){
								if(this.equals(st))
								{
									return smti;
								}
								if(smti.getLevel()>0||smti.getLevel()==-1)
