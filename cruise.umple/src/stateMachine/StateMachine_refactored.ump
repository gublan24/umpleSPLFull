								}
							}
							else if(smti.getIsIn()){
								if(tsn.getFromState().equals(this)){
									return smti;
								}
							}
							else {
								if(tsn.getFromState().equals(this)||tsn.getNextState().equals(this)){
									return smti;
								}
							}
						}
